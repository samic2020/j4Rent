	<queryString>
		<![CDATA[SELECT
	d.grupo,
	c.buffer,
	d.item,
	d.valor,
	d.data,
	d.autentica
FROM
	DESPESAS d,
	COMBOBUFFER c
WHERE
	c.autoid = d.grupo AND
	c.local = 'DESPES' AND
	(d.data >= $P{parameter1} AND
	d.data <= $P{parameter2}) AND
	IF($P{parameter3} = 0 , d.grupo != 0 , d.grupo = $P{parameter3})
ORDER BY
	d.grupo,
	d.data;]]>
